<h1>Graceland Diner</h1>

<h3>Eat A Burger, Thank You Very Much</h3>

<ul>
	{{#each burgers}}
	<li>
		<p>ID: {{this.id}}</p>
		<p>Task: {{this.burger}}</p>
	</li>
	{{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="task"></textarea>

	<button type="submit">Submit</button>
</form>